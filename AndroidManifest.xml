        </activity>
        <activity
            android:name=".customizedcontrols.PlayActivity"
            android:label="@string/title_activity_play" >
        </activity>
        <activity
            android:name=".listview.SimpleListViewActivity"
            android:label="@string/title_simple_list_view_main" >
