        <activity
            android:name=".listview.HeaderListViewActivity"
            android:label="@string/title_activity_header_list_view" >
        </activity>
        <activity
            android:name=".listview.CustomizeBaseAdapterActivity"
            android:label="@string/title_activity_customize_base_adapter" >
        </activity>
    </application>

</manifest>